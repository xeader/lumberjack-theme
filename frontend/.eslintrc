{
  "env": {
    "amd": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended", 
    "plugin:prettier/recommended", 
    "plugin:vue/recommended",
    "prettier"
  ],
  "globals": {
    "document": true,
    "window": true,
    "PATH_CONFIG": true
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  }
}
